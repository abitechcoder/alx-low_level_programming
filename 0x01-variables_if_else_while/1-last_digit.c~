#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/**
 * Main - print the last digit of the number stored in the variable n
 *
 * Return: Always (0) for success
 * 
 */

/* betty style doc for function main goes there */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	return (0);
}
